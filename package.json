{
  "name": "cadesignsystem",
  "version": "1.0.4",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "compile-css": "node-sass --output-style expanded  scss/_cabui.scss dist/css/cabui.css",
    "minify-css": "node-sass --output-style compressed scss/_cabui.scss dist/css/cabui.min.css",
    "compile-docs": "bundle exec jekyll build",
    "serve-docs": "bundle exec jekyll serve",
    "copy-assets": "mkdir dist/images/ && cp -R assets/images/* dist/images/",
    "dist-framework": "npm-run-all --parallel compile-css minify-css --sequential copy-assets"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "scss"
  ],
  "devDependencies": {
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3"
  }
}
