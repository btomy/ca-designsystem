  "scripts": {
    "build": "npm-run-all -p buildcss:expanded buildcss:compressed",
    "buildcss:expanded": "node-sass --output-style expanded --include-path ../../node_modules/ ./index.scss ./build/cads.system.css",
    "buildcss:compressed": "node-sass --output-style compressed --include-path ../../node_modules/ ./index.scss ./build/cads.system.min.css",
    "clean": "rm -rf ./build"
  }
}
