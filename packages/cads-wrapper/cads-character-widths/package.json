{
  "name": "cads-character-widths",
  "version": "0.0.0",
  "license": "MIT",
  "cads": {
    "component_type": "utility"
  },
  "files": [
    "index.scss",
    "build/",
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "cads-support": "0.0.0"
  },
  "scripts": {
    "build": "npm-run-all -p buildcss:expanded buildcss:compressed",
    "buildcss:expanded": "node-sass --output-style expanded --include-path ../../node_modules/ ./index.scss ./build/cads.character-widths.css",
    "buildcss:compressed": "node-sass --output-style compressed --include-path ../../node_modules/ ./index.scss ./build/cads.character-widths.min.css",
    "clean": "rm -rf ./build"
  }
}
