{
  "name": "@citizensadvice/font-size",
  "version": "1.0.1",
  "license": "MIT",
  "cads": {
    "package_type": "utility"
  },
  "files": [
    "index.scss",
    "build/",
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@citizensadvice/support": "^1.0.1"
  },
  "scripts": {
    "build": "npm-run-all -p buildcss:expanded buildcss:compressed",
    "buildcss:expanded": "node-sass --output-style expanded --include-path ../../node_modules/ ./index.scss ./build/font-size.css",
    "buildcss:compressed": "node-sass --output-style compressed --include-path ../../node_modules/ ./index.scss ./build/font-size.min.css",
    "clean": "rm -rf ./build"
  }
}
